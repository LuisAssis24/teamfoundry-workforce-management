\r\n        response.setContentType(MediaType.APPLICATION_JSON_VALUE);\r\n        response.setCharacterEncoding(StandardCharsets.UTF_8.name());\r\n        objectMapper.writeValue(response.getWriter(), payload);\r\n    }\r\n}\r\n
